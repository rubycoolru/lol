local Drawning = {
	["new"] = function(loc, tx, cl)
		local Gui = Instance.new("BillboardGui")
		Gui.Parent = loc
		Gui.AlwaysOnTop = true
		Gui.Name = "STexts"
		local SText = Instance.new("TextLabel")
		SText.Parent = Gui
		SText.BackgroundTransparency = 1
		SText.Text = tx
		SText.Name = "txt"
		if cl == nil then
			SText.TextColor3 = Color3.new(0,0,0)
		else
			SText.TextColor3 = cl
		end
	end
}
