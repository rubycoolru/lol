local Settings = {
	["Position"] = nil
}
game[Settings.Position].ChildAdded:Connect(function(i)
	local msg = function(txt)
		local gui = Instance.new("ScreenGui")
		gui.Parent = game.Players.LocalPlayer.PlayerGui
		gui.Name = "Notification"
		
		local Text = Instance.new("TextLabel")
		Text.Parent = gui
		Text.BackgroundColor3 = Color3.new(1,0,0)
		Text.BackgroundTransparency = 0.8
		Text.Position = UDim2.new(0.047, 0, 0.879, 0)
		Text.Size = UDim2.new(0.906, 0, 0.033, 0)
		Text.Text = txt.." Added"
		Text.TextScaled = true
		
		wait(2)
		game.TweenService:Create(Text, TweenInfo.new(0.5), {Transparency = 1})
		gui:Remove()
	end
	
	msg(i.Name)
end)
